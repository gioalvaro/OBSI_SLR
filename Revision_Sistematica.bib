journal = {International Journal of Computer Integrated Manufacturing},
keywords = {STEP-NC,ant colony optimisation,energy-efficient machining,ontology},
number = {4-5},
pages = {442--456},
title = {{A hybrid approach to energy-efficient machining for milled components via STEP-NC}},
volume = {31},
year = {2018}
}
