mendeley-tags = {introduccion},
pages = {140--150},
publisher = {Elsevier B.V.},
title = {{What does PLMS (product lifecycle management systems) manage: Data or documents? Complementarity and contingency for SMEs}},
volume = {75},
year = {2016}
}
